//Negation.hdl

/**
 * Negate a 16-bit number, be aware that first bit is the sign-bit. 
 * 2â€™s complement of the input 
 */

CHIP Negation {
    IN in[16];
    OUT out[16];

    PARTS:
	Not16(in = in, out = out1);
	Inc16(in = out1, out = out);
}